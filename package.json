    "@aws-sdk/client-cognito-identity-provider": "^3.81.0",
    "@aws-sdk/client-dynamodb": "^3.85.0",
    "@aws-sdk/util-dynamodb": "^3.85.0",
    "aws-cdk": "^2.23.0",
    "aws-cdk-lib": "^2.20.0",
    "aws-sdk": "^2.1125.0",
    "constructs": "^10.0.112",
    "source-map-support": "^0.5.21",
    "stripe": "^9.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.24.1",
    "@types/aws-lambda": "8.10.75",
    "@types/cookie": "0.4.1",
