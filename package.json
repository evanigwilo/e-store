{
  "name": "e-store-backend",
  "version": "0.1.0",
  "bin": {
    "e-store-backend": "bin/index.ts"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:ci": "jest --ci --runInBand --detectOpenHandles --coverage",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix",
