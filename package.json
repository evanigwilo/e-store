    "deploy": "cdk deploy --require-approval never --hotswap --outputs-file ./cdk-outputs.json"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.20.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.20.0-alpha.0",
